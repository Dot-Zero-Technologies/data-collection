<div class="form-control">
  <label for="<%= field.name %>"><%= field.label %></label>
  <input
    type="<%= field.type %>"
    name="<%= field.name %>"
    id="<%= field.name %>"
    <%= field.required ? 'required' : '' %>
    <% field.attributes.forEach((attr) => { %>
      <%= attr.name %>="<%= attr.value %>"
    <% }) %>
  />
</div>
